{
	"name": "@moonstack/encdec",
	"private": false,
	"version": "1.0.0",
	"type": "module",
	"files": [
		"dist",
		"index.d.ts"
	],
	"main": "./dist/encdec.js",
	"types": "./index.d.ts",
	"exports": {
		"types": "./index.d.ts",
		"require": "./dist/encdec.js"
	},
	"scripts": {
		"dev": "vite",
		"build": "rm -rf dist && tsc && swc lib -d dist",
